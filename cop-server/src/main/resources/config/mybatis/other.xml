<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cop.mobi.other.service.dao.OtherDao">
	<resultMap id="configResult" type="com.cop.mobi.other.entity.ConfigItem">
		<result property="platform" column="platform" />
		<result property="key" column="key" />
		<result property="value" column="value" />
	</resultMap>

	<select id="getConfig" resultMap="configResult">
		SELECT
		platform, `key`, `value`
		FROM
		LC_Config
	</select>

	<insert id="addFeedback">
		INSERT INTO LC_Feedback (uid, mcid, ua, content,
		addtime)
		VALUES (#{uid}, #{mcid}, #{ua}, #{content}, #{addtime})
	</insert>
</mapper>